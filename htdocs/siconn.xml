<?xml version="1.0" encoding="utf-8" ?>
<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="siconn" invoke="manual">
		<description>SmartInspect Connections property</description>
		<author>Gurock Software</author>
		<point name="connections">
			<text>file()</text>
			<hint>Specifies the connections to use.</hint>
		</point>
		<code language="C" context="methodbody" delimiter="|"><![CDATA[Si->Connections = "|connections|";]]></code>
	</template>
</codetemplate>
