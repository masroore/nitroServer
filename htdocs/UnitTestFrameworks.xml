  <UnitTestFramework>
    <Name>DUnit / Delphi Win32</Name>
    <OutputPersonality>Delphi.Personality</OutputPersonality>
    <BaseLocation>$(BDS)\Source\DUnit</BaseLocation>
    <SourceLocation>src</SourceLocation>
    <ExecutableLocation>bin</ExecutableLocation>
    <AddSourceLocation>false</AddSourceLocation>
    <CodeGenOptions>
      <CodeTemplate>DUnitTestCaseTemplate.pas</CodeTemplate>
      <Imports>TestFramework</Imports>
      <FileNamePrefix>Test</FileNamePrefix>
      <FileNameSuffix />
      <FunctionReturnVarName>ReturnValue</FunctionReturnVarName>
      <SetupMethodAttribute />
      <SetupMethodName>SetUp</SetupMethodName>
      <TearDownDisposeName>Free</TearDownDisposeName>
      <TearDownMethodName>TearDown</TearDownMethodName>
      <TearDownMethodAttribute />
      <TestCaseAttribute />
      <TestCaseBaseClass>TTestCase</TestCaseBaseClass>
      <TestCasePrefix>Test</TestCasePrefix>
      <TestCaseSuffix />
      <TestMethodAttribute />
      <TestMethodPrefix>Test</TestMethodPrefix>
      <TestMethodSuffix />
      <DataMemberNaming>FPrefix</DataMemberNaming>
      <TestClassComment>Test methods for class </TestClassComment>
      <TestMethodComment1>TODO: Setup method call parameters</TestMethodComment1>
      <TestMethodComment2>TODO: Validate method results</TestMethodComment2>
      <TestRegisterCode>RegisterTest(%s.Suite);</TestRegisterCode>
      <UsesAttributes>false</UsesAttributes>
    </CodeGenOptions>
    <ProjectOptions>
      <TemplateFile>DUnitTestProjectTemplate.dpr</TemplateFile>
      <ProjectExtension>.dpr</ProjectExtension>
      <ProjectType>Application</ProjectType>
      <References />
      <ProjectNamespace />
      <NeedsAssemblyInfo>false</NeedsAssemblyInfo>
    </ProjectOptions>
    <TestRunners>
      <UnitTestRunner>
        <Description>GUI</Description>
        <HostExecutable />
        <ConditionalDefines>_CONSOLE_TESTRUNNER</ConditionalDefines>
        <SearchPath />
      </UnitTestRunner>
      <UnitTestRunner>
        <Description>Console</Description>
        <HostExecutable />
        <ConditionalDefines>CONSOLE_TESTRUNNER</ConditionalDefines>
        <SearchPath />
      </UnitTestRunner>
    </TestRunners>
  </UnitTestFramework>
