<?xml version="1.0" encoding="utf-8" ?>
<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="sien" invoke="manual">
		<description>SmartInspect Enabled property</description>
		<author>Gurock Software</author>
		<point name="enabled">
			<text>true</text>
			<hint>Specifies if logging should be enabled or not.</hint>
		</point>
		<code language="C" context="methodbody" delimiter="|"><![CDATA[Si->Enabled = |enabled|;]]></code>
	</template>
</codetemplate>
