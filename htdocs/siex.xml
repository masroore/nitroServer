<?xml version="1.0" encoding="utf-8" ?>
<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="siex" invoke="manual">
		<description>SmartInspect LogException</description>
		<author>Gurock Software</author>
		<point name="exception">
			<text>E</text>
			<hint>The exception to log.</hint>
		</point>
		<code language="C" context="methodbody" delimiter="|"><![CDATA[SiMain->LogException(|exception|);]]></code>
	</template>
</codetemplate>

