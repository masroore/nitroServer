<?xml version="1.0" encoding="utf-8" ?>
<codetemplate	xmlns="http://schemas.borland.com/Delphi/2005/codetemplates"
				version="1.0.0">
	<template name="siverb" invoke="manual">
		<description>SmartInspect LogVerbose</description>
		<author>Gurock Software</author>
		<point name="title">
			<text>Title</text>
			<hint>The title of the verbose message.</hint>
		</point>
		<code language="C" context="methodbody" delimiter="|"><![CDATA[SiMain->LogVerbose("|title|");]]></code>
	</template>
</codetemplate>
